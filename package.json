{
  "name": "docutheques",
  "version": "1.0.0",
  "description": "Document libraries for your WordPress.",
  "engines": {
    "node": "10.13.0",
    "npm": "6.14.2"
  },
  "scripts": {
    "start": "parcel watch src/*/index.js --out-dir js --no-source-maps",
    "dev": "parcel build src/*/index.js --out-dir js --no-source-maps --no-minify",
    "build": "parcel build src/*/index.js --out-dir js",
		"wpcs": "./vendor/bin/phpcs class-docutheques.php inc/*.php templates/*.php --standard=WordPress",
		"app:cssmin": "uglifycss ./css/app.css > ./css/app.min.css",
		"widget:cssmin": "uglifycss ./css/widget.css > ./css/widget.min.css",
		"makepot": "wp i18n make-pot . languages/docutheques.pot --exclude=\".git,.cache,css,images,languages,node_modules,src,vendor\" --headers='{\"Project-Id-Version\": \"DocuTh√®ques\", \"Report-Msgid-Bugs-To\": \"https://github.com/imath/docutheques/issues\", \"Last-Translator\": \"imath <contact@imathi.eu>\", \"Language-Team\": \"FRENCH <contact@imathi.eu>\"}'",
		"makejson": "wp i18n make-json languages"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imath/docutheques.git"
  },
  "keywords": [
    "documents",
    "library"
  ],
  "author": "imath",
  "license": "GPL-2.0+",
  "bugs": {
    "url": "https://github.com/imath/docutheques/issues"
  },
  "homepage": "https://github.com/imath/docutheques#readme",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@wordpress/babel-preset-default": "^4.11.0",
    "parcel-bundler": "^1.12.4"
  },
  "dependencies": {
    "uglifycss": "0.0.29"
  }
}
